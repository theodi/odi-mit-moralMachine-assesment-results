{{! make the _globals object in course.json available to this template}}
{{import_globals}}

<div class="component__inner assessmentresults__inner">

  {{> component this}}

  <div class="component__widget assessmentresults__widget">

    {{#if _isRetryEnabled}}
    <div class="component__feedback assessmentresults__feedback">
      <div class="component__feedback-inner assessmentresults__feedback-inner">

        {{#if retryFeedback}}
        <div class="assessmentresults__retry-feedback">
          <div class="assessmentresults__retry-feedback-inner">
            {{{compile retryFeedback}}}
          </div>
        </div>
        {{/if}}
       <div class="moralMachineResults__container">
         <section id="results" style="display: block">
           <div id="top">
           {{#each _completionBody.preference}}
              {{{this}}} 
           {{/each}}
           </div>
         <sub-section class="left-50">
        {{#each _completionBody.leftArr}}
          {{{this}}}
        {{/each}}
         </sub-section> 
         <div id="padding" class="padding"></div>
         <sub-section class="right-50">
        {{#each _completionBody.rightArr}}
           {{{this}}}
        {{/each}}
         </sub-section>
        </section>
      </div>
        <button class="btn-text assessmentresults__retry-btn js-assessment-retry-btn">
          <span>
            {{#if _retry.button}}
            {{_retry.button}}
            {{else}}
            Retry
            {{/if}}
          </span>
        </button>

      </div>
    </div>
    {{/if}}

  </div>

</div>



